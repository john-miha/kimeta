
<div class="bg-warning p-3 p-sm-3 my-4 rounded">
  <div class="text-left text-white">
    <h1>メッセージ新規作成ページ</h1>
  </div>
</div>
<%= form_with(model: @chart) do |f| %>
    <%= f.label :title, "Title" %>
    <%= f.text_field :title %><br>
    <%= f.label :description, "Description" %>
    <%= f.text_field :description %><br>
    <%= f.submit 'submit' %>
<% end %>

<%= render 'evaluations/new', chart: @chart %>
  
<%= link_to '一覧に戻る', charts_path ,class: "btn btn-primary"%>