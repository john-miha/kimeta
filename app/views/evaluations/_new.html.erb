<%= form_with(model: chart.evaluations.build) do |f| %>
    <%= f.label :score, "Score" %>
    <%= f.text_field :score %><br>
    <%= f.label :comment, "Comment" %>
    <%= f.text_field :comment %><br>
    <%= f.label :itemname, "Item" %>
    <%= text_field_tag :itemname %><br>
    <%= f.label :viewpointname, "Viewpoint" %>
    <%= text_field_tag :viewpointname %><br>
    <%= f.submit 'submit' %>
<% end %>

